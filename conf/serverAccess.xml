<?xml version="1.0" encoding="UTF-8"?>

<accessList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<!-- The security roles.  By default, only "API" is used. -->
	<roles>
		<role name="API">
			<url>/api/*</url>
		</role>
	</roles>
	
	<!-- Users and passwords.  Passwords are stored as plaintext in this file. -->
	<users>
		<user username="test" password="test2">
			<role>API</role>
		</user>
	</users>
</accessList>
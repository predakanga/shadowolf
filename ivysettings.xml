<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
	<ivysettings>
    <settings defaultResolver="chained"/>
    
    <property name="java.net.maven.pattern" value="[organisation]/jars/[artifact]-[revision].[ext]"/>
 
    <resolvers>
        <chain name="chained" returnFirst="true">
        	<ibiblio name="gson" m2compatible="true"
        			root="http://google-gson.googlecode.com/svn/mavenrepo"/>
            <ibiblio name="ibiblio" m2compatible="true"/>
            <ibiblio name="java-net-maven2" 
            		root="http://download.java.net/maven/2/" 
            		m2compatible="true"/>
        </chain>
    </resolvers>
</ivysettings>

</ivysettings>